{
    "name": "Tabs Store",
    "version" : "1.0.0",
    "manifest_version" : 2,
    "update_url" : "http://upext.chrome.360.cn/intf.php?method=ExtUpdate.query",
    "icons": 
    { 
        "48": "icon-big.png",            
        "128": "icon-big.png" 
    }, 
    "description": "Store your chrome tabs and can be reuse on next time",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}